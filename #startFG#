    #!/bin/bash

fgfs --native-fdm='socket,out,30,localhost,5060,udp' \
	--native-ctrls='socket,in,30,,5070,udp' \
	--geometry=1024x768 \
	--disable-panel \
	--aircraft=f16 \
	--airport=KSAN \
	--disable-sound \
	--disable-hud  \
	--enable-clouds3d \
	--prop:/input/joysticks/js[0]/axis[0]/binding/command=null 


